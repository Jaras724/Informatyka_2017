#include <iostream>

using namespace std;

void wstaw (int T[], int ile) {

    for(int i=0; i<ile; i++) {
        cout<<"Podaj "<<i+1<<"element tablicy";
        cin>>T[i];
    }
}
void wyswietl (int T[], int ile) {
for (int i=0; i<ile; i++) {
    cout<<T[i]<<" ";
}
}

void wczytaj(int T[], int n) {
    for (int i=0; i<n; i++) {
        cout<<"T["<<i<<"] = ";
        cin>>T[i];
    }
}

int main()
{
    int T[5]={1,2,33,2,5};
    int tablica[10];
    wczytaj(tablica,10);
    cout<<endl;
    wyswietl(tablica,10);
}

