#include <iostream>
#include <cstdlib>
#include <cmath>
#include <iomanip>

using namespace std;
//prze≈Çadowanie funkcji

int obliczSume(int a){
    return a;
}

int obliczSume(int a, int b){
    return a+b;
}

int obliczSume(int a, int b, int c) {
return a+b+c;
}

int main()
{

    int x=2, y=5, z=10;
    cout<<obliczSume(z)<<endl;
    cout<<obliczSume(z,x)<<endl;
    cout<<obliczSume(z,z,x)<<endl;

return 0;
}
